# ArchitectAI Platform Environment Variables
# Copy this file to .env and fill in your API keys

# Google Maps API Key
# Get one from: https://console.cloud.google.com/
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenWeather API Key
# Get one from: https://openweathermap.org/api
REACT_APP_OPENWEATHER_API_KEY=your_openweather_api_key_here

# OpenAI API Key
# Get one from: https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Replicate API Key
# Get one from: https://replicate.com/account/api-tokens
REACT_APP_REPLICATE_API_KEY=your_replicate_api_key_here

# Optional: Enable debug logging in production
# REACT_APP_DEBUG=true

# Optional: API rate limits (requests per minute)
# REACT_APP_OPENAI_RATE_LIMIT=20
# REACT_APP_REPLICATE_RATE_LIMIT=10

# Optional: Timeout settings (milliseconds)
# REACT_APP_API_TIMEOUT=30000
# REACT_APP_IMAGE_GENERATION_TIMEOUT=60000
